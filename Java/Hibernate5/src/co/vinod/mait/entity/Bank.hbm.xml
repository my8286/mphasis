<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="co.vinod.mait.entity">
	
	<!-- class to table mapping -->
	<class name="Bank" table="Bankaccount">

		<id name="accno" column="accno" type="int" />
		<property name="name" column="name" type="string" />
		<property name="balance" column="balance" type="int" />
		<property name="phoneno" column="phoneno" type="string" />
		<property name="email" column="email" type="string"/>
		

	</class>
	
	
</hibernate-mapping>
